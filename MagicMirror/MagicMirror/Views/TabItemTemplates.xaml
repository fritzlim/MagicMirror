<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:v="clr-namespace:Baku.MagicMirror.Views"
                    xmlns:vm="clr-namespace:Baku.MagicMirror.ViewModels">
    <DataTemplate DataType="{x:Type vm:QiConnectionServiceViewModel}">
        <v:ConnectionView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:CameraMonitorViewModel}">
        <v:CameraView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ChatDialogViewModel}">
        <v:ChatDialogView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:PluginTabViewModel}">
        <ContentControl Content="{Binding Plugin.GuiContent}"/>
    </DataTemplate>
</ResourceDictionary>